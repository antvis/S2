{
  "name": "@antv/s2-site",
  "private": true,
  "description": "S2 sites deployed on gh-pages",
  "version": "0.1.0",
  "keywords": [
    "antv",
    "s2",
    "spreadsheet",
    "site"
  ],
  "license": "MIT",
  "homepage": "https://s2.antv.vision",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/s2"
  },
  "bugs": {
    "url": "https://github.com/antvis/s2/issues"
  },
  "scripts": {
    "start": "yarn develop",
    "develop": "gatsby develop --open -H 0.0.0.0",
    "build": "yarn clean && gatsby build --no-uglify --prefix-paths",
    "clean": "gatsby clean",
    "preview": "gatsby serve",
    "deploy": "yarn build && gh-pages -d public"
  },
  "resolutions": {
    "monaco-editor-webpack-plugin": "2.0.0",
    "monaco-editor": "0.21.0",
    "sharp": "0.29.0"
  },
  "dependencies": {
    "@antv/gatsby-theme-antv": "^1.1.8",
    "gatsby": "^2.24.2",
    "gh-pages": "^3.1.0",
    "ptz-i18n": "^1.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-i18next": "^11.5.0",
    "@antv/s2": "^0.1.13"
  }
}
