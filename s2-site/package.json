{
  "name": "@antv/s2-site",
  "private": true,
  "description": "S2 sites deployed on gh-pages",
  "version": "0.1.0",
  "keywords": ["antv", "s2", "spreadsheet", "site"],
  "license": "MIT",
  "homepage": "https://s2.antv.vision",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/s2"
  },
  "bugs": {
    "url": "https://github.com/antvis/s2/issues"
  },
  "scripts": {
    "start": "yarn develop",
    "develop": "gatsby develop --open -H 0.0.0.0",
    "build": "yarn clean && gatsby build --no-uglify --prefix-paths",
    "clean": "gatsby clean",
    "preview": "gatsby serve",
    "deploy": "yarn build && gh-pages -d public",
    "sync:s2-lock": "yarn upgrade @antv/s2 @antv/s2-react --latest"
  },
  "resolutions": {
    "monaco-editor-webpack-plugin": "2.0.0",
    "monaco-editor": "0.21.0",
    "sharp": "0.29.0",
    "normalize-url": "^4.5.1"
  },
  "dependencies": {
    "@antv/g-canvas": "^0.5.12",
    "@antv/gatsby-theme-antv": "1.1.18-beta.0",
    "@antv/s2": "^1.14.0-alpha.1",
    "@antv/s2-react": "^1.13.0-alpha.1",
    "copy-to-clipboard": "^3.3.1",
    "gatsby": "^2.24.2",
    "gh-pages": "^3.1.0",
    "lodash": "^4.17.21",
    "ptz-i18n": "^1.0.0",
    "react": "^16.10.2",
    "react-color": "^2.19.3",
    "react-dom": "^16.10.2",
    "react-i18next": "^11.5.0"
  }
}
