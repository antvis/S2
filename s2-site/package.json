{
  "name": "@antv/s2-site",
  "private": true,
  "description": "S2 sites deployed on gh-pages",
  "version": "0.1.0",
  "keywords": [
    "antv",
    "s2",
    "spreadsheet",
    "site"
  ],
  "license": "MIT",
  "homepage": "https://s2.antv.antgroup.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/s2"
  },
  "bugs": {
    "url": "https://github.com/antvis/s2/issues"
  },
  "scripts": {
    "start": "yarn develop",
    "develop": "dumi dev",
    "build": "dumi build",
    "clean": "rimraf .dumi",
    "preview": "dumi preview",
    "deploy": "dumi build &&  cp ./CNAME ./dist/CNAME && gh-pages -d dist",
    "sync:s2-lock": "yarn upgrade @antv/s2 @antv/s2-react --latest"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.1",
    "@antv/dumi-theme-antv": "^0.4.3",
    "@antv/g-canvas": "^0.5.12",
    "@antv/s2": "^1.52.0",
    "@antv/s2-react": "^1.44.2",
    "antd": "^4.24.14",
    "copy-to-clipboard": "^3.3.3",
    "dumi": "^2.2.14",
    "gh-pages": "^3.1.0",
    "lodash": "^4.17.21",
    "react-color": "^2.19.3"
  },
  "devDependencies": {
    "@google-cloud/translate": "^7.0.3",
    "hast-util-to-html": "^8.0.3",
    "micromark-extension-directive": "^2.1.2",
    "rehype-document": "^6.1.0",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-remark": "^9.1.2",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "tnpm": {
    "mode": "yarn",
    "lockfile": "enable"
  }
}
