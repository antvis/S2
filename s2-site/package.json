{
  "name": "@antv/s2-site",
  "private": true,
  "description": "S2 sites deployed on gh-pages",
  "version": "0.1.0",
  "keywords": [
    "antv",
    "s2",
    "spreadsheet",
    "site"
  ],
  "license": "MIT",
  "homepage": "https://s2.antv.vision",
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/s2"
  },
  "bugs": {
    "url": "https://github.com/antvis/s2/issues"
  },
  "scripts": {
    "start": "npm run site:develop",
    "site:develop": "gatsby develop --open -H 0.0.0.0",
    "site:build": "npm run site:clean && gatsby build --prefix-paths",
    "site:clean": "gatsby clean",
    "site:deploy": "npm run site:build && gh-pages -d public"
  },
  "dependencies": {
    "@antv/gatsby-theme-antv": "1.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "gatsby": "^2.24.2",
    "gh-pages": "^3.1.0",
    "ptz-i18n": "^1.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-i18next": "^11.5.0"
  }
}
