{
  "name": "@antv/s2",
  "version": "0.1.11",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/s2.min.js",
  "types": "esm/index.d.ts",
  "description": "effective spreadsheet render core lib",
  "license": "MIT",
  "homepage": "https://s2.antv.vision",
  "author": "https://github.com/orgs/antvis/people",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/S2.git"
  },
  "bugs": {
    "url": "https://github.com/antvis/S2/issues"
  },
  "files": [
    "esm",
    "lib",
    "dist",
    "README.md"
  ],
  "keywords": [
    "antv",
    "s2",
    "spreadsheet",
    "pivot table"
  ],
  "scripts": {
    "start": "yarn test:live",
    "clean": "rimraf lib esm dist",
    "build": "run-s clean build:*",
    "build:esm": "FORMAT=esm rollup -c rollup.config.js",
    "build:cjs": "FORMAT=cjs rollup -c rollup.config.js",
    "build:umd": "FORMAT=umd rollup -c rollup.config.js",
    "bundle:size": "bundlesize",
    "watch": "rimraf esm && FORMAT=es rollup -w -c rollup.config.js",
    "test:live": "node ./scripts/test-live.mjs",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "tsc": "tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "@antv/event-emitter": "~0.1.2",
    "@antv/g-canvas": "^0.5.12",
    "@antv/g-gesture": "^1.0.0",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react-beautiful-dnd": "^13.1.0"
  },
  "devDependencies": {
    "@types/react-beautiful-dnd": "^13.1.2",
    "d3-dsv": "^1.1.1",
    "inquirer": "^8.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "sideEffects": [
    "*.css",
    "dist/*"
  ],
  "bundlesize": [
    {
      "path": "./dist/s2.min.js",
      "maxSize": "120 kB"
    },
    {
      "path": "./dist/s2.min.css",
      "maxSize": "5 kB"
    }
  ]
}
